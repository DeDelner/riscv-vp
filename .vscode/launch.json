{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/riscv-vp/sw/simple-unreal/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "riscv32-unknown-elf-objdump",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": false
                }
            ],
        }
    ]
}